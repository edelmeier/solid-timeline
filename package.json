{
  "name": "@edelmeier/solid-timeline",
  "version": "0.0.1",
  "description": "UI Components for basic date interaction in the solidjs ecosystem",
  "main": "dist/cjs/my-lib.js",
  "module": "dist/esm/my-lib.js",
  "type": "module",
  "types": "dist/types/my-lib.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/my-lib.js",
      "require": "./dist/my-lib.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx --cache src",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edelmeier/solid-timeline.git"
  },
  "keywords": [
    "solidjs",
    "date-picker",
    "date",
    "time",
    "calendar"
  ],
  "author": "Sebastian Edelmeier",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edelmeier/solid-timeline/issues"
  },
  "homepage": "https://github.com/edelmeier/solid-timeline#readme",
  "dependencies": {
    "@suid/icons-material": "*",
    "@suid/material": "*",
    "solid-js": "*"
  },
  "devDependencies": {
    "@suid/vite-plugin": "*",
    "typescript": "^4.8.2",
    "vite": "*",
    "vite-plugin-solid": "*",
    "eslint-plugin-solid": "^0.9.1",
    "prettier": "^2.8.2",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1"
  }
